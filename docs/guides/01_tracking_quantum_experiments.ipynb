     "data": {
      "text/plain": [
       "<Trial [Example trial] 88b5ef54-a66b-45e1-aed8-c7db7ad82594>"
      ]
     },
     "execution_count": 7,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "local_backend.get([0])"
    # same as local_backend.get(\"88b5ef54-a66b-45e1-aed8-c7db7ad82594\")",
   ]
  }
